{
    "name": "mxl/laravel-api-key",
    "description": "API Key Authorization for Laravel with replay attack prevention",
    "keywords": [
        "api",
        "key",
        "authorization",
        "authentication",
        "middleware",
        "replay",
        "attack",
        "timestamp",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "https://github.com/mxl/laravel-api-key",
    "support": {
        "issues": "https://github.com/mxl/laravel-api-key/issues",
        "source": "https://github.com/mxl/laravel-api-key"
    },
    "type": "library",
    "require": {
        "php": ">=7.1",
        "laravel/framework": "5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "authors": [
        {
            "name": "Michael Ledin",
            "email": "mledin89@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MichaelLedin\\LaravelApiKey\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichaelLedin\\LaravelApiKey\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichaelLedin\\LaravelApiKey\\ApiKeyServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
